# Ignore node_modules for both client and server
/node_modules
/client/node_modules
/server/node_modules

# Ignore package lock files (can be included if you want to track exact dependency versions)
package-lock.json
/client/package-lock.json
/server/package-lock.json

# Angular specific build and cache files
/client/dist
/client/.angular
/client/.angular/cache
/client/.angular/cache/*

# Vite-specific build and cache files
/client/.vite
/client/vite

# Ignore compiled JavaScript files from TypeScript
/client/src/**/*.js
/client/src/**/*.js.map

# Angular build output (production and development)
.client/dist

# Ignore environment-specific files
/client/src/environments/environment.*.ts

# Ignore TypeScript build info
/client/tsconfig.tsbuildinfo
/client/tsconfig*.json

# VSCode specific settings
.vscode

# Ignore temporary system files
.DS_Store
Thumbs.db

# Ignore logs and debug files
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE or editor-specific files
.idea
.project
.settings
*.sublime-workspace

# Ignore lock files generated by build tools
*.lock

# Ignore compiled output
/client/build
/client/out

# Ignore Git-specific files
.git

# Ignore sensitive files such as secrets, API keys, etc.
.env
.env.local
.env.*.local
*.secret
*.key

# Ignore any specific generated or compiled artifacts from the backend
/server/build
/server/.cache
